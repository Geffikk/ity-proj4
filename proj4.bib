%5 Cast publikace Logaritmy
@article{MIYAKE20161,
title = "Logarithmically homogeneous preferences",
journal = "Journal of Mathematical Economics",
volume = "67",
pages = "1 - 9",
year = "2016",
issn = "0304-4068",
doi = "https://doi.org/10.1016/j.jmateco.2016.08.005",
url = {https://www.sciencedirect.com/science/article/pii/S0304406816301021},
author = "Mitsunobu Miyake",
keywords = "Difference comparison, Intensity comparison, Logarithmically homogeneous utility function, Additively separable logarithmic utility function, Stone’s price index",
abstract = "An extended-real-valued function on R+n is called logarithmically homogeneous if it is given by the logarithmic transformation of a homogeneous function on R+n. Specifying a consumer’s preference on the consumption set by a difference comparison relation, this paper provides some axioms on the relation under which the full class of utility functions representing the relation are logarithmically homogeneous. It is also shown that all the utility functions are strongly concave and all the indirect utility functions are logarithmically homogeneous. Moreover, the additively separable logarithmic utility functions are derived by strengthening one of the axioms."
}

